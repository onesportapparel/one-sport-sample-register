
{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post", "patch"],
      "route": "bookings/{id?}/{subaction?}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "bookingsIn",
      "connectionStringSetting": "CosmosDBConnectionString",
      "databaseName": "OneSportDB",
      "collectionName": "Bookings"
    },
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "bookingsOut",
      "connectionStringSetting": "CosmosDBConnectionString",
      "databaseName": "OneSportDB",
      "collectionName": "Bookings"
    }
  ]
}
